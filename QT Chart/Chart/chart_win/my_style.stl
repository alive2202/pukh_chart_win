QPushButton
{
 border: 2px solid black;
 border-radius: 0px;

 border-right-color: gray;
 border-bottom-color: gray;

 border-left-color: lightgray;
 border-top-color: lightgray;

 color: black;
 font: bold 11px;
 font-family: Tahoma;

 background-color: qlineargradient(x1:0, y1:0, x2:0, y2:1, stop : 0 #bebebe, stop:1 #d2d1d0);
}

QPushButton#btnTR_MoveL,#btnTR_MoveR,#btnVO_ManPrev,#btnVO_ManNext
{
 font: bold 14px;
 font-family: Arial;
}

QPushButton:pressed
{
 border: 2px solid black;
 border-radius: 0px;

 border-right-color: lightgray;
 border-bottom-color: lightgray;

 border-left-color: darkgray;
 border-top-color: darkgray;

 color: lightgray;
 font: bold 11px;

 background-color: qlineargradient(x1:0, y1:0, x2:0, y2:1, stop : 0 #008b00, stop:1 #006000);
}

QPushButton:checked
{
 border: 2px solid black;
 border-radius: 0px;

 border-right-color: lightgray;
 border-bottom-color: lightgray;

 border-left-color: darkgray;
 border-top-color: darkgray;

 color: lightgray;
 font: bold 11px;

 background-color: qlineargradient(x1:0, y1:0, x2:0, y2:1, stop : 0 #008b00, stop:1 #006000);
}

QPushButton:disabled
{
 background-color: qlineargradient(x1:0, y1:0, x2:0, y2:1, stop : 0 #c5c5c5, stop:1 #c0c0c0);
 color: gray;
}

